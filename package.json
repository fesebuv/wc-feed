{
  "name": "wc-feed",
  "version": "0.0.1",
  "description": "wc-feed",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf src/public/css && mkdir src/public/css",
    "build": "webpack && npm-sass ./src/client/styles/styles.scss > ./src/public/css/styles.css",
    "coverage": "jest --coverage",
    "prestart": "npm run build",
    "start": "${NODE_BIN:-node} index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fesebuv/wc-feed.git"
  },
  "keywords": [
    "wc",
    "worldcup"
  ],
  "author": "Felipe S. Buena√±o",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/fesebuv/wc-feed/issues"
  },
  "homepage": "https://github.com/fesebuv/wc-feed#readme",
  "dependencies": {
    "express": "^4.16.3",
    "node-fetch": "^2.1.2",
    "querystring": "^0.2.0",
    "vue": "^2.5.16",
    "whatwg-fetch": "^2.0.4"
  },
  "private": "true",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.21",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^23.4.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-vue": "^0.8.2",
    "jsdom": "^11.11.0",
    "npm-sass": "^2.3.0",
    "vue-jest": "^2.6.0",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": [
      "<rootDir>/test/unit/setup"
    ]
  }
}
